<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>
		<server>
			<id>public</id>
			<username>deployment</username>
			<password>cinexus@2014</password>
		</server>
		<server>
			<id>snapshots</id>
			<username>deployment</username>
			<password>cinexus@2014</password>
		</server>
		<server>
			<id>releases</id>
			<username>deployment</username>
			<password>cinexus@2014</password>
		</server>
	</servers>

	<profiles>
		<profile>
			<id>nexus</id>
			<repositories>
				<repository>
					<id>public</id>
					<url>http://192.168.0.41:8081/nexus/content/groups/public/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
				</repository>
				<repository>
					<id>snapshots</id>
					<url>http://192.168.0.41:8081/nexus/content/repositories/snapshots/</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>releases</id>
					<url>http://192.168.0.41:8081/nexus/content/repositories/releases/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>

			</repositories>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<downloadSources>false</downloadSources>
				<downloadJavadocs>false</downloadJavadocs>
			</properties>
			<pluginRepositories>
				<pluginRepository>
					<id>public</id>
					<url>http://192.168.0.41:8081/nexus/content/groups/public/</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>nexus</activeProfile>
	</activeProfiles>

</settings>

